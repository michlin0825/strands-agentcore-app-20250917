# AWS Configuration
AWS_PROFILE=your_aws_profile
AWS_REGION=us-east-1

# Bedrock AgentCore Configuration
AGENT_RUNTIME_ARN=arn:aws:bedrock-agentcore:us-east-1:ACCOUNT_ID:runtime/YOUR_RUNTIME_NAME
KNOWLEDGE_BASE_ID=YOUR_KNOWLEDGE_BASE_ID

# Cognito Authentication
COGNITO_USER_POOL_ID=your_user_pool_id
COGNITO_CLIENT_ID=your_client_id
COGNITO_USERNAME=your_username_here
COGNITO_PASSWORD=your_password_here

# Optional: Direct AWS Credentials (alternative to AWS_PROFILE)
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_SESSION_TOKEN=your_session_token_here
